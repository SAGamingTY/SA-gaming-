                    LeftArm = LeftArm,
                    RightArm = RightArm,
                    LeftLeg = LeftLeg,
                    RightLeg = RightLeg
                }
            end
        end
    end

    function PlayerUtilities:GetDistanceFromClient(Position)
        local ClientBodyParts = PlayerUtilities:GetBodyParts(LocalPlayer)

